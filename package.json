{
  "name": "sumer-sdk",
  "version": "4.2.0",
  "description": "A package that allows devs to track the errors that may occur in EVM-based Dapps",
  "main": "build/module/index.js",
  "typings": "build/module/index.d.ts",
  "module": "build/module/index.js",
  "files": [
    "./build/"
  ],
  "scripts": {
    "build": " NODE_ENV=production npm run  build:main &&  NODE_ENV=production npm run  build:module",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "dev": "tsc -p tsconfig.module.json -w",
    "test": "jest --colors",
    "build:contracts": "npx waffle",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wagmi-VS/sumer-sdk.git"
  },
  "keywords": [],
  "author": {
    "name": "Wagmi Venture Studio"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wagmi-VS/sumer-sdk/issues"
  },
  "homepage": "https://github.com/Wagmi-VS/sumer-sdk#readme",
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@openzeppelin/contracts": "^4.7.3",
    "axios": "^0.27.2",
    "babel-jest": "^27.1.5",
    "bowser": "^2.11.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.0"
  },
  "bundleDependencies": [
    "@ethersproject/abi",
    "@ethersproject/providers",
    "axios",
    "babel-jest",
    "bowser",
    "ethers"
  ],
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^18.7.18",
    "ethereum-waffle": "^4.0.6",
    "jest": "^27.5.1",
    "jest-ts-auto-mock": "^2.1.0",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.3"
  }
}
